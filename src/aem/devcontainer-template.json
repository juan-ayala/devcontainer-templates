{
    "id": "aem",
    "version": "1.0.1",
    "name": "Adobe Experience Manager",
    "description": "Develop AEM applications.",
    "documentationURL": "https://github.com/juan-ayala/devcontainer-templates/tree/main/src/aem",
    "licenseURL": "https://github.com/juan-ayala/devcontainer-templates/blob/main/LICENSE",
    "options": {
        "imageVariant": {
            "type": "string",
            "description": "Container image variant",
            "proposals": [
                "debian",
                "ubuntu"
            ],
            "default": "debian"
        },
        "nodeJsVersion": {
            "type": "string",
            "description": "Node.js version to install.",
            "default": "12.22.7"
        }
    },
    "platforms": [
        "Any"
    ]
}
